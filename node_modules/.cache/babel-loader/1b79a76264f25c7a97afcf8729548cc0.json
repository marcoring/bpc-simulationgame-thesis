AAA,KAAA,CAAA,SAAA,KAAA,CAAA,EAAA;AACA,aAAA,SAAA;AACA,OAFA,MAEA,IAAA,KAAA,MAAA,CAAA,KAAA,CAAA,SAAA,KAAA,CAAA,EAAA;AACA,aAAA,cAAA;AACA,OAFA,MAEA;AACA,aAAA,UAAA;AACA;;AAEA,WAAA,MAAA,CAAA,KAAA,CAAA,SAAA;AACA,KApDA;AAqDA,IAAA,QArDA,sBAqDA;AACA,WAAA,QAAA,GAAA,oKAAA;AAEA,WAAA,KAAA,CAAA,sBAAA;AAEA,WAAA,UAAA,CAAA,YAAA,EAAA,CAAA;AACA,WAAA,UAAA,CAAA,OAAA,EAAA,GAAA;AACA,WAAA,UAAA,CAAA,YAAA,EAAA,GAAA;AAEA,WAAA,SAAA,CAAA,YAAA;AACA,KA/DA;AAgEA,IAAA,SAhEA,uBAgEA;AACA,WAAA,QAAA,GAAA,+CAAA;AAEA,WAAA,UAAA,CAAA,OAAA,EAAA,CAAA;AACA,WAAA,UAAA,CAAA,YAAA,EAAA,GAAA;AAEA,WAAA,SAAA,CAAA,OAAA;AACA,WAAA,WAAA,CAAA,YAAA;AACA,KAxEA;AAyEA,IAAA,cAzEA,4BAyEA;AACA,WAAA,QAAA,GAAA,+GAAA;AAEA,WAAA,UAAA,CAAA,YAAA,EAAA,CAAA;AACA,WAAA,UAAA,CAAA,OAAA,EAAA,GAAA;AAEA,WAAA,SAAA,CAAA,YAAA;AACA,WAAA,WAAA,CAAA,OAAA;AACA,KAjFA;AAkFA,IAAA,UAlFA,wBAkFA;AACA,WAAA,WAAA,CAAA,YAAA;AACA,WAAA,UAAA,CAAA,YAAA,EAAA,CAAA;AACA,WAAA,UAAA,CAAA,OAAA,EAAA,CAAA;AAEA,WAAA,KAAA,CAAA,sBAAA;AACA,KAxFA;AAyFA,IAAA,SAzFA,qBAyFA,IAzFA,EAyFA;AACA,WAAA,KAAA,CAAA,IAAA,EAAA,KAAA,CAAA,MAAA,GAAA,eAAA;AACA,WAAA,KAAA,CAAA,IAAA,EAAA,KAAA,CAAA,YAAA,GAAA,MAAA;AACA,KA5FA;AA6FA,IAAA,WA7FA,uBA6FA,IA7FA,EA6FA;AACA,WAAA,KAAA,CAAA,IAAA,EAAA,KAAA,CAAA,MAAA,GAAA,KAAA;AACA,KA/FA;AAgGA,IAAA,UAhGA,sBAgGA,IAhGA,EAgGA,KAhGA,EAgGA;AACA,WAAA,KAAA,CAAA,IAAA,EAAA,KAAA,CAAA,OAAA,GAAA,KAAA;AACA;AAlGA,GAtCA;AA0IA,EAAA,OA1IA,qBA0IA;AACA,SAAA,MAAA,CAAA,KAAA,CAAA,cAAA,GACA,KAAA,MAAA,CAAA,KAAA,CAAA,cAAA,IAAA,CAAA,IACA,KAAA,MAAA,CAAA,KAAA,CAAA,YAAA,IAAA,CADA,IAEA,KAAA,MAAA,CAAA,KAAA,CAAA,SAAA,IAAA,CAFA,IAGA,KAAA,MAAA,CAAA,KAAA,CAAA,UAAA,IAAA,CAHA,IAIA,KAAA,MAAA,CAAA,KAAA,CAAA,QAAA,IAAA,CAJA,IAKA,KAAA,MAAA,CAAA,KAAA,CAAA,SAAA,IAAA,CANA;;AAQA,QAAA,KAAA,MAAA,CAAA,KAAA,CAAA,cAAA,EAAA;AACA,WAAA,MAAA,CAAA,KAAA,CAAA,SAAA,GAAA,CAAA;AACA;;AAEA,QAAA,KAAA,MAAA,CAAA,KAAA,CAAA,SAAA,IAAA,CAAA,EAAA;AACA,WAAA,kBAAA;AACA;AACA;AA1JA,CAAA","sourcesContent":["<template>\n  <v-container id=\"frame-preparation\" >\n    <!-- custom component with statistic about current, previous round and cost accounting -->\n    <div ref=\"round-data\">\n      <prev-cur-round-stats\n        :prevAsmLine=\"'SmartLine'\"\n        :prevAsmLineCost=\"0.0\"\n        :prevNumOfAsmLines=\"0\"\n        :prevProdCosts=\"0.0\"\n        :prevProdCapac=\"0.0\"\n        :prevQuality=\"0.0\"\n        :prevWorkload=\"0.0\"\n        :prevSafety=\"0.0\"\n        :curAsmLine=\"'SmartLine'\"\n        :curAsmLineCost=\"0.0\"\n        :curNumOfAsmLines=\"0\"\n        :curProdCosts=\"0.0\"\n        :curProdCapac=\"0.0\"\n        :curQuality=\"0.0\"\n        :curWorkload=\"0.0\"\n        :curSafety=\"0.0\"\n        :budget=\"150000.0\"\n        :runningCosts=\"0.0\"\n        :avgProdCostBike=\"'Incomplete'\"\n        :estimatedQual=\"0.0\"\n        :maxProdCapac=\"'Incomplete'\"\n        :overDemand=\"40000.0\"\n        style=\"height: 500px;\"\n      />\n    </div>\n\n    <v-divider />\n\n    <v-row v-if=\"this.$store.state.frameStep <= 4\" class=\"pa-2\" style=\"margin-top: 20px; margin-bottom: 40px;\">\n      <v-col align=\"left\" cols=\"9\">\n        <div>\n          <h2>{{ this.stepText }}</h2>\n        </div>\n      </v-col>\n      <v-col align=\"right\">\n        <v-btn @click=\"nextPurchasingStep\" dark rounded link :color=\"teamColor\">\n          <b>I understand</b>\n        </v-btn>\n      </v-col>\n    </v-row>\n\n    <!-- Managing frame preparation process -->\n    <div ref=\"logic\" style=\"margin: 1px;\">\n      <v-row style=\"margin-top: 15px; margin-left: 3px\">\n        <h2 style=\"text-align: left;\">Manage frame preparation process</h2>\n      </v-row>\n      <v-row>\n        <v-col>\n          <v-select\n            v-model=\"selectedLine\"\n            :items=\"assemblyLines\"\n            :color=\"teamColor\"\n            label=\"Choose assembly line...\"\n            item-text=\"name\"\n          />\n\n          <v-text-field\n            label=\"Assembly costs (EUR)\"\n            :value=\"calculateCosts(selectedLine[0])\"\n            filled\n            disabled\n          />\n          <v-text-field\n            label=\"Production costs (EUR)\"\n            :value=\"calculateCosts(selectedLine[1])\"\n            filled\n            disabled\n          />\n          <v-text-field\n            label=\"Production capacity (PC)\"\n            :value=\"calculateCosts(selectedLine[2])\"\n            filled\n            disabled\n          />\n        </v-col>\n\n        <v-col>\n          <v-slider\n            v-model=\"numOfLines\"\n            label=\"Number of Assembly Lines\"\n            step=\"1\"\n            :min=\"1\"\n            :max=\"10\"\n            ticks=\"always\"\n            tick-size=\"5\"\n            thumb-label=\"always\"\n            :color=\"teamColor\"\n            :thumb-color=\"teamColor\"\n            :track-color=\"'teamColor' + 'lighten-3'\"\n            :track-fill-color=\"teamColor\"\n            :thumb-size=\"24\"\n          >\n            <template v-slot:append>\n              <v-text-field\n                v-model=\"numOfLines\"\n                class=\"mt-0 pt-0\"\n                hide-details\n                single-line\n                :min=\"1\"\n                :max=\"10\"\n                type=\"number\"\n                style=\"width: 60px\"\n              />\n            </template>\n          </v-slider>\n\n          <v-slider\n            v-model=\"quality.val\"\n            :label=\"quality.label\"\n            :color=\"teamColor\"\n            :min=\"1\"\n            :max=\"100\"\n            :thumb-color=\"teamColor\"\n            :thumb-size=\"24\"\n            thumb-label=\"always\"\n            :track-color=\"'teamColor' + 'lighten-3'\"\n            :track-fill-color=\"teamColor\"\n          >\n            <template v-slot:append>\n              <v-text-field\n                v-model=\"quality.val\"\n                class=\"mt-0 pt-0\"\n                hide-details\n                single-line\n                :min=\"1\"\n                :max=\"100\"\n                type=\"number\"\n                style=\"width: 60px\"\n              />\n            </template>\n          </v-slider>\n\n          <v-slider\n            v-model=\"workload.val\"\n            :label=\"workload.label\"\n            :color=\"teamColor\"\n            :thumb-color=\"teamColor\"\n            :min=\"1\"\n            :max=\"100\"\n            :thumb-size=\"24\"\n            thumb-label=\"always\"\n            :track-color=\"'teamColor' + 'lighten-3'\"\n            :track-fill-color=\"teamColor\"\n          >\n            <template v-slot:append>\n              <v-text-field\n                v-model=\"workload.val\"\n                class=\"mt-0 pt-0\"\n                hide-details\n                single-line\n                :min=\"1\"\n                :max=\"100\"\n                type=\"number\"\n                style=\"width: 60px\"\n              />\n            </template>\n          </v-slider>\n\n          <v-slider\n            v-model=\"safety.val\"\n            :label=\"safety.label\"\n            :color=\"teamColor\"\n            :thumb-color=\"teamColor\"\n            :min=\"1\"\n            :max=\"100\"\n            :thumb-size=\"24\"\n            thumb-label=\"always\"\n            :track-color=\"'teamColor' + 'lighten-3'\"\n            :track-fill-color=\"teamColor\"\n          >\n            <template v-slot:append>\n              <v-text-field\n                v-model=\"safety.val\"\n                class=\"mt-0 pt-0\"\n                hide-details\n                single-line\n                :min=\"1\"\n                :max=\"100\"\n                type=\"number\"\n                style=\"width: 60px\"\n              />\n            </template>\n          </v-slider>\n        </v-col>\n      </v-row>\n    </div>\n\n   <v-row ref=\"navigation\">\n        <v-col md='4' align=\"left\">\n          <v-btn :color=\"teamColor\" rounded dark @click=\"toPreviousStep\">\n            <v-icon left>mdi-arrow-left-bold-circle-outline</v-icon>\n            Previous step\n          </v-btn>\n        </v-col>\n\n        <v-col align=\"center\" md=\"4\">\n          <v-btn :color=\"teamColor\" rounded dark @click=\"toggleDialog\" center>\n            <v-icon left>mdi-check-outline</v-icon>\n            Save changes\n          </v-btn>\n        </v-col>\n      \n        <v-col md=\"4\" align=\"right\">\n          <v-btn :color=\"teamColor\" rounded dark right @click=\"toNextStep\" >\n            <v-icon left>mdi-arrow-right-bold-circle-outline</v-icon>\n            Next step\n          </v-btn>\n        </v-col>\n    </v-row>\n    <confirmation-dialog \n      v-if=\"confirmChangesDialog\"\n      @closeDialog=\"toggleDialog\"\n      @updateProgress=\"updateProgress\"\n    ></confirmation-dialog>\n    <error-chages-dialog\n      v-if=\"showError\"\n      @closeError=\"toggleShowError\"\n    ></error-chages-dialog>\n  </v-container>\n</template>\n\n<script>\nimport prevCurRoundStats from \"../components/prevCurRoundStats.vue\";\nimport ConfirmationDialog from \"../dialogs/ConfirmationDialog.vue\";\nimport ErrorChagesDialog from '../dialogs/ErrorChagesDialog.vue';\n\nexport default {\n  components: { prevCurRoundStats, ConfirmationDialog, ErrorChagesDialog },\n  name: \"frame-preparation\",\n  data() {\n    return {\n      showError: false,\n      stepText: '',\n      confirmChangesDialog: false,\n      selectedLine: \"\",\n      numOfLines: 1,\n      quality: { label: \"Quality (%)\", val: 50, color: \"primary\" },\n      workload: { label: \"Workload (%)\", val: 50, color: \"primary\" },\n      safety: { label: \"Safety (%)\", val: 50, color: \"primary\" },\n      assemblyLines: [\n        {\n          name: \"Assembly Line 1\",\n          value: [\"100\", \"50\", \"200\"],\n        },\n        {\n          name: \"Assembly Line 2\",\n          value: [\"300\", \"100\", \"600\"],\n        },\n        {\n          name: \"Assembly Line 3\",\n          value: [\"500\", \"250\", \"2000\"],\n        },\n      ],\n      teamColor: this.$store.state.color\n    };\n  },\n  watch: {\n    '$store.state.frameStep': function() {\n      if(this.$store.state.frameStep === 0) {\n        this.$store.state.frameStep++;\n        this.nextPurchasingStep();\n      }\n    }\n  },\n  methods: {\n    toggleShowError() {\n      this.showError = !this.showError;\n    },\n    toggleDialog() {\n      if(this.selectedLine === \"\") {\n        this.toggleShowError();\n      } else if(this.$store.state.frameStep >= 5){\n        this.confirmChangesDialog = !this.confirmChangesDialog;\n      }\n    },\n    updateProgress() {\n      this.$emit(\"updateProgress\", \"framePreparation\", 100);\n      //this.$router.push({ path: \"/dashboard\" });\n      this.toggleDialog();\n    },\n    calculateCosts(selectedLine) {\n      // check for NaN\n      if (typeof selectedLine === \"undefined\") {\n        return \"\";\n      } else {\n        return (\n          selectedLine *\n          this.numOfLines *\n          (1 + this.quality.val / 100) *\n          (1 + this.workload.val / 100) *\n          (1 + this.safety.val / 100)\n        ).toFixed(2);\n      }\n    },\n    toNextStep() {\n      if(this.$store.state.frameStep >= 5) {\n        this.$router.push({ path: \"/sensorsPreparation\" });\n      }\n    },\n    toPreviousStep() {\n      if(this.$store.state.frameStep >= 5) {\n        this.$router.push({ path: \"/logistics\" });\n      }\n    },\n     nextPurchasingStep() {\n       if(this.$store.state.frameStep === 1) {\n        this.dataStep();\n      } else if(this.$store.state.frameStep === 2) {\n        this.logicStep();\n      } else if(this.$store.state.frameStep === 3) {\n        this.navigationStep();\n      } else {\n        this.unlockStep();\n      }\n\n      this.$store.state.frameStep++;\n    },\n    dataStep() {\n      this.stepText = 'In these tables, you can find useful information to previous and current round. On the left side is the previous round and on the right side is the current round.';\n\n      this.$emit('toggleMenuVisability');\n\n      this.setOpacity(\"round-data\", 1);\n      this.setOpacity(\"logic\", 0.3);\n      this.setOpacity(\"navigation\", 0.3);\n\n      this.setBorder(\"round-data\");\n    },\n    logicStep() {\n      this.stepText = 'Here ypu can configure your frame production!'\n\n      this.setOpacity(\"logic\", 1);\n      this.setOpacity(\"round-data\", 0.3);\n\n      this.setBorder(\"logic\");\n      this.resetBorder(\"round-data\");\n    },\n    navigationStep() {\n      this.stepText = 'You can also navigate through different steps. Do not forget that the steps may differ depending on the round'\n\n      this.setOpacity(\"navigation\", 1);\n      this.setOpacity(\"logic\", 0.3);\n\n      this.setBorder(\"navigation\");\n      this.resetBorder(\"logic\");\n    },\n    unlockStep() {\n      this.resetBorder(\"navigation\");\n      this.setOpacity(\"round-data\", 1);\n      this.setOpacity(\"logic\", 1);\n\n      this.$emit('toggleMenuVisability');\n    },\n    setBorder(name) {\n      this.$refs[name].style.border = '5px solid red';\n      this.$refs[name].style.borderRadius = '20px';\n    },\n    resetBorder(name) {\n      this.$refs[name].style.border = '0px';\n    },\n    setOpacity(name, value) {\n      this.$refs[name].style.opacity = value;\n    },\n  },\n  mounted() {\n    this.$store.state.innerGuideDone = \n               this.$store.state.purchasingStep >= 5 || \n               this.$store.state.logisticStep >= 5 || \n               this.$store.state.frameStep >= 5 || \n               this.$store.state.sensorStep >= 5 || \n               this.$store.state.bikeStep >= 5 || \n               this.$store.state.salesStep >= 5;\n\n    if(this.$store.state.innerGuideDone) {\n      this.$store.state.frameStep = 5;\n    }\n\n    if(this.$store.state.frameStep <= 4) {\n      this.nextPurchasingStep();\n    }  \n  },\n};\n</script>\n"],"sourceRoot":"src/views"}]}