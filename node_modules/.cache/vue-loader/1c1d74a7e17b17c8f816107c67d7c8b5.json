xt=\"item.icon\" v-if=\"item.value < 100\" :color=\"teamColor\"></v-icon>\n                        <v-icon v-else>mdi-check-outline</v-icon>\n                    </v-list-item-icon>\n                    <v-list-item-content \n                        class=\"text-item\" \n                        @click=\"navigateToItem(item.id)\"\n                        :style=\"calculateStyle(item.value)\"\n                    >\n                       <span >{{item.name}}</span>\n                    </v-list-item-content>\n                    </v-list-item>\n                </v-list-item-group>\n                </v-list>\n          </v-card-text>\n\n          <v-divider />\n\n          <v-card-actions>\n            <v-spacer />\n            <v-btn color=\"red\" text @click=\"closeDialog\">\n              <b>Close</b>\n            </v-btn>\n          </v-card-actions>\n        </v-card>\n      </v-dialog>\n</template>\n\n<script>\n\nexport default {\n  props: ['tasksList'],\n   data () {\n      return {\n        dialog: true,\n        teamColor: this.$store.state.color\n      }\n    },\n    methods: {\n        closeDialog() {\n            this.dialog = false;\n            this.$emit(\"closeTodo\");\n        },\n        navigateToItem(itemId) {\n            this.closeDialog();\n            this.$router.push({ path: \"/\" + itemId });\n        },\n        calculateStyle(itemValue) {\n            const normalItemStyle = 'color: ' + this.teamColor + '; font-weight: 600;';\n           return itemValue == 100 ? '' : normalItemStyle;\n        }\n    }\n}\n</script>\n\n<style scoped>\n    .text-item {\n        text-align: left;\n        font-size: 15pt;\n    }\n</style>\n"]}]}